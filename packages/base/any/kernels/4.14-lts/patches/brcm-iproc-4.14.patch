-			usage(argv[0], EXIT_FAILURE);
-		else
-			read_ec_val(fd, read_byte_offset);
-	else
-		write_ec_val(fd, write_byte_offset, write_value);
-	close(fd);
-
-	exit(EXIT_SUCCESS);
-}
