+static int apm_ioctl(struct net_device *net_dev, struct ifreq *ifr, int cmd)
+{
+	if (!netif_running(net_dev))
+		return -EINVAL;
+
+	return phy_mii_ioctl(net_dev->phydev, ifr, cmd);
+}
