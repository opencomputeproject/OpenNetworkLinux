+struct iproc_usb_priv {
+	struct usb_phy phy;
+	struct phy_device *mdio_phy;
+	void __iomem *wrap_base;
+	void __iomem *idm_base;
+	void __iomem *utmi_base;
+	int usb_mode;
+	int init_count;
+};
