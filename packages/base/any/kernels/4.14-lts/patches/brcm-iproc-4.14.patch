+
+static bool apm_wait_value(struct apm *apm, u16 reg, u32 mask,
+			     u32 value, int timeout)
+{
+	u32 val;
+	int i;
+
+	for (i = 0; i < timeout / 10; i++) {
+		val = apm_read(apm, reg);
+		if ((val & mask) == value)
+			return true;
+		udelay(10);
+	}
+	dev_err(apm->dev, "Timeout waiting for reg 0x%X\n", reg);
+	return false;
+}
