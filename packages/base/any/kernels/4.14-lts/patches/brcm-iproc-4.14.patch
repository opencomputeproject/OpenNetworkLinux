+ * Generic platform device driver definition.
+ */
+static struct platform_driver xgs_iproc_udc_driver =
+{
+	.probe      = xgs_iproc_udc_probe,
+	.remove     = xgs_iproc_udc_remove,
+	.driver = {
+		.name   = XGS_IPROC_UDC_NAME,
+		.owner  = THIS_MODULE,
+		.of_match_table = xgs_iproc_udc_ids,
+	},
+};
