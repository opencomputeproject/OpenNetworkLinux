+#define DECLARE_DPAA_RESOURCE(name)			\
+struct dpaa_resource name = {				\
+	.free = LIST_HEAD_INIT(name.free),		\
+	.lock = __SPIN_LOCK_UNLOCKED(name.lock),	\
+	.used = LIST_HEAD_INIT(name.used)               \
+}
