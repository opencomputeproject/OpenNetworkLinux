+static struct i2c_driver as5812_54t_sfp_driver = {
+    .class        = I2C_CLASS_HWMON,
+    .driver = {
+        .name     = "as5812_54t_sfp",
+    },
+    .probe        = as5812_54t_sfp_probe,
